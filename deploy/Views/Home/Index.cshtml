@model GastosAppMvc.Infrastructure.HomeHolder
@{
    ViewBag.Title = "MoneyTrace";
}

<h2>@ViewBag.Message</h2>
@section scripts {
<script>
    $(function () {
        //$("#okmessage").hide("slide",5000);
        $("#okmessage").fadeOut(4000, "easeInExpo");
        $("#okmessage").corner();
    });
    </script>    
        @Scripts.Render("~/bundles/flot")
    <!--[if IE]><script src="@Url.Content("~/Scripts/excanvas.min.js")"></script><![endif]-->
    <script src="@Url.Content("~/Scripts/app/jsReportUtil.js")" type="text/javascript"></script>

<script>
    $(function () {
        var data = parseTableData($("#tblReporte"), -1);
        $.plot($("#grafica"), data,
	    {
	        series: {
	            pie: {
	                show: true
	            }
	        },
	        grid: {
	            hoverable: true,
	            clickable: false
	        }
	    });

        $("#grafica").bind("plothover", pieHover);

    });
</script>
}
 <p>
    MoneyTrace
    <div class="Message" id="okmessage">Bienvenido</div> 
    
    <div style="width:100%; ">
    @if (User.Identity.IsAuthenticated)
    {
    @Html.Partial("_SumConcepto", Model.RepSumConceptoHolder)
    }
    </div>
</p>

